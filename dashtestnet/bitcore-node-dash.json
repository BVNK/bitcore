{
  "network": "testnet",
  "port": 3001,
  "services": [
    "bitcoind",
    "insight-api-dash",
    "insight-ui-dash",
    "web"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "/root/.dashcore/data",
        "exec": "/root/.dash/bin/dashd"
      },
      "connect": [{
        "rpchost": "127.0.0.1",
        "rpcport": 9998,
        "rpcuser": "rpcuser",
        "rpcpassword": "rpcpassword",
        "zmqpubrawtx": "tcp://127.0.0.1:28332"
      }]
    }
  }
}