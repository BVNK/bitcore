{
  "network": "testnet",
  "port": 3001,
  "services": [
    "litecoind",
    "web",
    "insight-api",
    "insight-ui"
  ],
  "servicesConfig": {
    "litecoind": {
      "spawn": {
        "datadir": "/root/.litecore/data",
        "exec": "/usr/local/lib/node_modules/litecore/node_modules/litecore-node/bin/litecoind"
      },
      "insight-api": {
  "disableRateLimiter": true,
        "rateLimiterOptions": {
        "limit":  1000000000000
      }
      }
    }
  }
}
